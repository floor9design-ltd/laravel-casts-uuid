{
    "name": "floor9design/laravel-casts-uuid",
    "type": "library",
    "description": "A class for casting ramsey/uuid into laravel models",
    "keywords": [
        "floor9design",
        "laravel",
        "casts",
        "uuid"
    ],
    "homepage": "https://github.com/floor9design-ltd/laravel-casts-uuid",
    "license": "MIT",
    "authors": [
        {
            "name": "Rick Morice",
            "email": "rick@floor9design.com",
            "homepage": "http://floor9design.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^9.0",
        "php" : ">=8.0",
        "ramsey/uuid" : "^4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0.0",
        "phpunit/phpunit": "^9",
        "ramsey/uuid": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Floor9design\\LaravelCasts\\": "src",
            "Floor9design\\LaravelCasts\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
